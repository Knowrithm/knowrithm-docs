
{ 
  "title": "Knowrithm Documentation",
  "description": "Complete guide to building intelligent AI agents with Knowrithm platform",
  "author": "Knowrithm Team",
  "language": "en",
  "gitbook": ">=3.0.0",
  
  "plugins": [
    "highlight",
    "search",
    "fontsettings@^2.0.0",
    "theme-default",
    "-sharing",
    "-livereload"
  ],
  
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "fontsettings": {
      "theme": "white",
      "family": "sans",
      "size": 2
    }
  },
  
  "styles": {
    "website": "styles/website.css"
  },
  
  "variables": {
    "version": "0.1.3",
    "company": "Knowrithm",
    "apiUrl": "https://pypi.org/project/knowrithm-py",
    "dashboardUrl": "https://www.knowrithm.com"
  },
  
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  }
}