{
  "title": "Knowrithm Documentation",
  "description": "Complete guide to building intelligent AI agents with the Knowrithm platform.",
  "author": "Knowrithm Team",
  "language": "en",
  "gitbook": ">=3.2.0",
  "plugins": [
    "highlight",
    "search",
    "lunr",
    "fontsettings",
    "theme-default",
    "-sharing",
    "-livereload"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "fontsettings": {
      "theme": "white",
      "family": "sans",
      "size": 3
    },
    "lunr": {
      "maxIndexSize": 1000000,
      "fuzziness": 2,
      "distance": 100
    }
  },
  "styles": {
    "website": "styles/websites.css"
  },
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "variables": {
    "version": "1.0.0",
    "company": "Knowrithm",
    "apiUrl": "https://app.knowrithm.org/api/v1",
    "dashboardUrl": "https://app.knowrithm.org"
  }
}
